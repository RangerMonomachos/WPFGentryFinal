<Window x:Class="WPFGentryFinal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFGentryFinal"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <TextBox Name="TextBoxText" HorizontalAlignment="Left" Height="23" Margin="192,141.75,0,0" TextWrapping="Wrap" Text="Type" VerticalAlignment="Top" Width="120"/>

        <TextBlock Name="outputReal" HorizontalAlignment="Left" Margin="396.666,148.79,0,0" TextWrapping="Wrap" Text="{Binding ElementName=TextBoxText,Path=Text,UpdateSourceTrigger=PropertyChanged}" VerticalAlignment="Top"/>
        <Button Name="GoForm3" Content="Form 3" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="GoForm3_Click"/>
        <Button Name="GoForm2" Content="Form 2" HorizontalAlignment="Left" Margin="702,10,0,0" VerticalAlignment="Top" Width="75" Click="GoForm2_Click"/>
        <Button Name="CloseAll" Content="End Program" HorizontalAlignment="Left" Margin="347,10,0,0" VerticalAlignment="Top" Width="90" Click="CloseAll_Click"/>

    </Grid>
</Window>
